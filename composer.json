{
    "name": "javaabu/activitylog",
    "description": "Customized version of spatie/laravel-activitylog",
    "homepage": "https://github.com/Javaabu/activitylog",
    "license": "MIT",
    "authors": [
        {
            "name": "Arushad Ahmed",
            "email": "arushad@javaabu.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0",
        "spatie/laravel-activitylog": "^4.7",
        "javaabu/helpers": "^1.2"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0",
        "phpunit/phpunit": "^9.5 || ^10.5"
    },
    "autoload": {
        "psr-4": {
            "Javaabu\\Activitylog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javaabu\\Activitylog\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "ActivitylogServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
